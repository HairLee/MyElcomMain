<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <com.example.arc.view.custom.HomeFragmentCalendarView
            android:id="@+id/homeFragmentCalendarView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        </com.example.arc.view.custom.HomeFragmentCalendarView>

        <com.example.arc.view.custom.LunchRegistrationContentView
            android:id="@+id/lunchRegistrationContentView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        </com.example.arc.view.custom.LunchRegistrationContentView>

        <EditText
            android:id="@+id/btnEdt"
            android:layout_width="match_parent"
            android:layout_height="40dp" />


    </LinearLayout>
</layout>